  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "pwTestApp-browser:chrome": "npx playwright test pwTestApp --project=chromium",
    "globalsQaApp-browser:firefox": "npx playwright test globalsQaApp --project=firefox",
    "globalsQaApp-browser:webkit": "npx playwright test globalsQaApp --project=webkit",
    "pwTestApp-headed": "npx playwright test pwTestApp --project=chromium --headed",
    "pwTestApp-ui": "npx playwright test pwTestApp --project=chromium --ui",
    "pwTestApp-debug": "npx playwright test pwTestApp --project=chromium --debug",
    "pwTestApp-type:smoke": "npx playwright test pwTestApp --project=chromium -g \"@smoke\" ",
    "pwTestApp-type:all": "npx playwright test pwTestApp --project=chromium -g \"@smoke|@regression|@sanity|@e2e|@integration\"",
    "pwTestApp-frequency:daily": "npx playwright test pwTestApp --project=chromium -g \"@daily\"",
    "pwTestApp-frequency:weekly": "npx playwright test pwTestApp --project=chromium -g \"@weekly\"",
    "pwTestApp-platform:mobile": "npx playwright test pwTestApp --project=mobile",
    "uiTestingPlayground-env:dev": "npx playwright test uiTestingPlayground --project=chromium \"@dev\"",
    "uiTestingPlayground-env:staging": "npx playwright test uiTestingPlayground --project=chromium \"@staging\"",
    "uiTestingPlayground-env:prod": "npx playwright test uiTestingPlayground --project=chromium -g \"@prod\"",
    "pwTestApp-env:prod": "npx playwright test pwTestApp --project=chromium --config=playwright-prod.config.ts",
    "pwTestApp-lastFailed": "npx playwright test pwTestApp --last-failed",
    "uiTestingPlayground-prio:critical": "npx playwright test uiTestingPlayground --project=chromium -g \"@critical\"",
    "uiTestingPlayground-prio:all": "npx playwright test uiTestingPlayground --project=chromium -g \"@critical|@high|@medium|@low\""
  },
